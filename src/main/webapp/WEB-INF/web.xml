<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
         xmlns="https://java.sun.xom/xml/ns/javaee"
         xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://java.sun.com/xml/ns/javaee
https://java.sun.com/xml/ns/javaee/web-app_3_0.xsd
https://www.w3.org/2001/XMLSchema-instance
https://java.sun.xom/xml/ns/javaee ">

    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>org.example.MyServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ServletDB</servlet-name>
        <servlet-class>org.example.ServletDB</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>org.example.ProfileServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ServletDB</servlet-name>
        <url-pattern>/data</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ProfileServlet</servlet-name>
        <url-pattern>/profiles</url-pattern>
    </servlet-mapping>

</web-app>